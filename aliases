# IP addresses
alias ip="dig +short myip.opendns.com @resolver1.opendns.com"
alias fn="find . -name"

# Runs matlab from the command-line without the interface
#
# NOTE: You need to put the matlab binary into your path
#   ln -s /Applications/YourMatlab.app/bin/matlab /usr/bin/matlab
alias matlab="matlab -nodesktop"
